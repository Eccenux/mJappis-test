<?xml version="1.0" encoding="UTF-8" ?>
<widget xmlns = "http://www.w3.org/ns/widgets"
	xmlns:gap = "http://phonegap.com/ns/1.0"
	id        = "mjappis.TestProject"
	versionCode="00101"
	version   = "0.1.1">
	<!--
		versionCode = Mmmbb : Major.minor.build
	-->
	
    <name>mJappis test</name>

    <description>
        This is just a test.
    </description>

    <author href="http://www.mjappis.org" email="eccenux+mjex@gmail.com">
        Maciej Nux Jaros
    </author>

	<!-- Mobile OS to be build -->
	<gap:platform name="android" />

	<!-- PG version -->
	<preference name="phonegap-version" value="cli-6.3.0" />

	<!--
		plugins
		
		info:
		<del>https://build.phonegap.com/plugins</del>
		https://cordova.apache.org/plugins/?platforms=cordova-android%2Ccordova-ios
	-->
	
	<!-- icons -->
	<icon src="logo/logo128.png" default="true" />
	<icon src="logo/logo036.png" width="36"  height="36"/>
	<icon src="logo/logo048.png" width="48"  height="48"/>
	<icon src="logo/logo057.png" width="57"  height="57"/>
	<icon src="logo/logo072.png" width="72"  height="72"/>
	<icon src="logo/logo080.png" width="80"  height="80"/>
	<icon src="logo/logo114.png" width="114" height="114"/>

	<!--
		Permissions.
		
		We don't really need specific permissions,
		but PGB will add some from plugins.
	-->
	<preference name="permissions" value="none"/>
	
</widget>